on boot
    chown system system  /dev/goodix_fp
    chmod 0644 /dev/goodix_fp

service gx_fpd_fix /vendor/bin/sh /vendor/etc/gx_fpd_fix.sh
    class post-fs-data
#    seclabel u:object_r:shell_exec:s0
    user root
    group shell
    oneshot
    disabled

on post-fs-data
    chown root root  /vendor/etc/gx_fpd_fix.sh
    chmod 0777 /vendor/etc/gx_fpd_fix.sh
    start gx_fpd_fix

service gx_fpd /vendor/bin/gx_fpd
    class late_start
    user system
    group system
